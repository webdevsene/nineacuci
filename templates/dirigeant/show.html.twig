{% extends 'base.html.twig' %}

{% block title %}Dirigeant{% endblock %}

{% block body %}
    <h1>Dirigeant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dirigeant.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ dirigeant.createdAt ? dirigeant.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ dirigeant.updatedAt ? dirigeant.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Addresse</th>
                <td>{{ dirigeant.addresse }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ dirigeant.nom }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ dirigeant.position }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ dirigeant.prenom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('dirigeant_index') }}">back to list</a>

    <a href="{{ path('dirigeant_edit', {'id': dirigeant.id}) }}">edit</a>

    {{ include('dirigeant/_delete_form.html.twig') }}
