{% extends 'base.html.twig' %}

{% block title %}New QVH{% endblock %}

{% block body %}




<!-- ici on ajoute le data-table liste ressource -->
<div class="row">

    <!-- end le data-table liste region -->


    
    <div class="row">
        <div class="col-xl-6">
            <div class="card" style=" margin: 0%;padding: 0%;-webkit-box-shadow:0 1rem 3rem rgba(0,0,0,.175);box-shadow:0 1rem 3rem rgba(0,0,0,.175)">
                <div class="card-body">
<!--                    
 -->
                    <div class="table-responsive">
                        <table id="usertable" class="table table-hover mb-0">

                            <thead>
                                <tr>
                                   
                                    <th data-priority="3">Code CACR </th>
                                    <th data-priority="3">Libelle</th>
                                    
                                </tr>
                            </thead>
                            
                            <tbody>
                                {% for tag in cacr %}
                                <tr>
                                   
                                    <td>{{ tag.codeCacr }}</td>
                                    <td>{{ tag.libelle}}</td>
                                   
                                </tr>
                                {% else %}
                                        <tr>
                                            <td colspan="12">Aucun enregistrement trouv√©</td>
                                        </tr>
                                {% endfor %}
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
        
        <div class="col-xl-6">
            <div class="card" style=" margin: 0%;padding: 0%;-webkit-box-shadow:0 1rem 3rem rgba(0,0,0,.175);box-shadow:0 1rem 3rem rgba(0,0,0,.175)">
                <div class="card-body">
                      
                    <div class="row">
                        <!-- begin to upload file -->
                        <form  enctype="multipart/form-data" class="form-horizontal d-flex align-items-center justify-content-right" method="post" action="{{ path('importCacr')}}">
                                     <div class="form-group">
                                        <!-- <label for="_file" class="control-label col-sm-3">Charger le template</label> -->
                                        <div class="col-sm-9">
                                            <input class="form-control" type="file" name="_file" id="_file" title="Charger un template">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-6-offset-2">
                                          <input type="submit" class="btn btn-secondary" value="Charger">
                                        </div>
                                     </div>
                        </form>
                            
                </div> <!-- end col -->

                </div>
            </div>
        </div>
    </div>
    <!-- end row -->

{% endblock %}

